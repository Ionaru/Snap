{
  "name": "snap",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "lint": "tslint --project tsconfig.json -c tslint.json",
    "start": "npm run build && node dist/snap.js",
    "start:ts": "node --require ts-node/register src/snap.ts",
    "start:docker": "npm run docker-build && npm run docker-run",
    "docker-build": "docker build . --no-cache -t snap",
    "docker-run": "npm run docker-stop && docker run --detach --name snap snap",
    "docker-stop": "(docker stop snap && docker rm snap) || exit 0"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "puppeteer": "git+https://github.com/GoogleChrome/puppeteer.git",
    "winston-pnp-logger": "^1.0.4"
  },
  "devDependencies": {
    "@types/ini": "^1.3.29",
    "@types/node": "^8.5.2",
    "@types/puppeteer": "^0.13.9",
    "@types/winston": "^2.3.7",
    "ts-node": "^4.0.2",
    "tslint": "^5.8.0",
    "typescript": "^2.6.2"
  }
}
